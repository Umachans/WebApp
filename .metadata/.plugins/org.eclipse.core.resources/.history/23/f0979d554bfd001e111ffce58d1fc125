#DB(MySQL)接続情報

spring.application.name=MySampleWeb
spring.datasource.url=jdbc:mysql://localhost/spring_div
spring.datasource.username=root
spring.datasource.password=umanatsuki1002
spring.datasource.driver-class-name=com.mysql.cj.jdbc.Driver

#Spring Security
#アカウントロックを行うログイン失敗回数境界値
security.locking-border-count=3
#アカウントロックの持続時間
security.locking-time=1

